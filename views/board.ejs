<h2><%=bt[0].title%></h2>
<table class="table table-bordered border-primary">
    <thead>
        <td>작성자</td>
        <td>제목</td>
        <td>날짜</td>
    </thead>
    <%
    var i = 0;
    while(i < list.length){%>
        <tr>
            <td><%=list[i].loginid%></td>
            <td><%=list[i].title%></td>
            <td><%=list[i].date%></td>
        </tr>
        <%
        i++;
    }%>
</table>

<%
if((bt[0].write_YN === 'Y' && v === 'Y') || c === '01'){%>
    <button class="btn btn-info me-md-2" style="float: right;" type="button" onclick="location.href='/board/create/<%=bt[0].type_id%>'">글쓰기</button>
<%}%>

